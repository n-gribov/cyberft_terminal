<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://cyberft.ru/xsd/edm.02" targetNamespace="http://cyberft.ru/xsd/edm.02" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:data="http://cyberft.ru/xsd/cftdata.02" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:import namespace="http://cyberft.ru/xsd/cftdata.02" schemaLocation="CyberFT_Data_v2.4.xsd"/>
  <xs:element name="StatementRequest">
    <xs:annotation>
      <xs:documentation>Запрос на формировании выписки</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="StatementAccount" type="AccountType">
          <xs:annotation>
            <xs:documentation>Номер счета, по которому запрашивается выписка</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="StatementPeriod" type="ReportPeriodType">
          <xs:annotation>
            <xs:documentation>Период, за который запрашивается выписка</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="Id" type="xs:ID" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Statement">
    <xs:annotation>
      <xs:documentation>Выписка</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="StatementNumber" type="xs:string">
          <xs:annotation>
            <xs:documentation>Номер выписки</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="CreateDate" type="xs:dateTime">
          <xs:annotation>
            <xs:documentation>Дата + Время формирования выписки</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="StatementAccount">
          <xs:annotation>
            <xs:documentation>Счет, по которому сформирована выписка</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:complexContent>
              <xs:extension base="AccountType">
                <xs:sequence>
                  <xs:element name="AccountName" type="xs:string" minOccurs="0"/>
                </xs:sequence>
              </xs:extension>
            </xs:complexContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="StatementPeriod" type="ReportPeriodType">
          <xs:annotation>
            <xs:documentation>Период за который сформирована выписка</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="OpeningBalance" type="xs:decimal">
          <xs:annotation>
            <xs:documentation>Входящий остаток на счете</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DebitTurnover" type="xs:decimal">
          <xs:annotation>
            <xs:documentation>Дебетовый оборот по счету</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="CreditTurnover" type="xs:decimal">
          <xs:annotation>
            <xs:documentation>Кредитовый оборот по счету</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ClosingBalance" type="xs:decimal">
          <xs:annotation>
            <xs:documentation>Исходящий остаток на счете</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="RefDoc" type="RefDocType" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Ссылка на запрос на выписку</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="PayDoc" type="PayDocType" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>Движение по счету</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="AddInfo" type="AddInfoType" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="Id" type="xs:ID" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PaymentStatusReport">
    <xs:annotation>
      <xs:documentation>Статусы обработки платежных поручений</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="RefDoc" type="RefDocType" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Ссылка на исходное сообщение</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="GroupStatus" type="GroupStatusType">
          <xs:annotation>
            <xs:documentation>Статус реестра платежей</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="Id" type="xs:ID" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PaymentRegister">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header">
          <xs:annotation>
            <xs:documentation>Заголовок реестра платежей</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="RegisterId" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Id реестра платежей</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RegisterDate" type="xs:dateTime">
                <xs:annotation>
                  <xs:documentation>Дата+ время формирования реестра платежей</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PaymentCount" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>Количество платежей в реестре</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PaymentSum" type="RegisterSumType">
                <xs:annotation>
                  <xs:documentation>Сумма и валюта платежей в реестре</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RegisterDetails" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Дополнительная информация по реестру платежей</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="Id" type="xs:ID" use="optional"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="Content">
          <xs:annotation>
            <xs:documentation>Контейнер реестра платежей (призвольный тип данных)</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:any namespace="##any" processContents="lax" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="Id" type="xs:ID" use="optional"/>
            <xs:attribute name="format" type="xs:string" use="required">
              <xs:annotation>
                <xs:documentation>Формат реестра платежей (SWIFT, 1C и т.д.)</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:anyAttribute namespace="##any" processContents="lax"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="Attachments" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Контейнер приложений к реестру</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="AttachmentRef" type="data:AttachmentRefType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Ссылка на приложение к реестру</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="Id" type="xs:ID" use="optional"/>
            <xs:anyAttribute namespace="##any" processContents="lax"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="Signatures">
          <xs:annotation>
            <xs:documentation>Контейнер подписей (ЭЦП подписантов)</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="Id" type="xs:ID" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ForeignCurrencyPurchaseRequest">
    <xs:annotation>
      <xs:documentation>Заявление на покупку иностранной валюты</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Number" type="xs:string">
          <xs:annotation>
            <xs:documentation>Номер заявки на продажу валюты</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Date" type="xs:date">
          <xs:annotation>
            <xs:documentation>Дата заявки на продажу валюты</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Applicant" type="ApplicantType">
          <xs:annotation>
            <xs:documentation>Заявитель</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="PaymentOrder">
          <xs:annotation>
            <xs:documentation>Валютное платежное поручение</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:complexContent>
              <xs:extension base="PaymentOrderType">
                <xs:sequence>
                  <xs:element name="PurchaseJustification" type="PurchaseJustificationType">
                    <xs:annotation>
                      <xs:documentation>Основание на покупку иностранной валюты</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:extension>
            </xs:complexContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="ValidUntil" type="xs:date" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Срок действия заявления</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="data:Attachments" minOccurs="0"/>
        <xs:element ref="data:Signatures"/>
      </xs:sequence>
      <xs:attribute name="Id" type="xs:ID" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ForeignCurrencySellRequest">
    <xs:annotation>
      <xs:documentation>Заявление на продажу иностранной валюты</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Number" type="xs:string">
          <xs:annotation>
            <xs:documentation>Номер заявки на продажу валюты</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Date" type="xs:date">
          <xs:annotation>
            <xs:documentation>Дата заявки на продажу валюты</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Applicant" type="ApplicantType">
          <xs:annotation>
            <xs:documentation>Заявитель</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="PaymentOrder" type="PaymentOrderType">
          <xs:annotation>
            <xs:documentation>Валютное платежное поручение</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ValidUntil" type="xs:date" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Срок действия заявления</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="data:Attachments" minOccurs="0"/>
        <xs:element ref="data:Signatures"/>
      </xs:sequence>
      <xs:attribute name="Id" type="xs:ID" use="optional"/>
    </xs:complexType>
  </xs:element>
  <!-- ///////////////////////////////////////////////////////////////// -->
  <xs:complexType name="RegisterSumType">
    <xs:simpleContent>
      <xs:extension base="xs:double">
        <xs:attribute name="currency" type="xs:string" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="ReportPeriodType">
    <xs:sequence>
      <xs:element name="StartDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>Дата начала периода</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EndDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>Дата конца периода</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AccountType">
    <xs:sequence>
      <xs:choice>
        <xs:sequence>
          <xs:element name="AccountNumber" type="AccNumType">
            <xs:annotation>
              <xs:documentation>Номер банковского счета РФ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BIK" type="BicType">
            <xs:annotation>
              <xs:documentation>БИК банка РФ</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:sequence>
          <xs:element name="IBAN" type="IBAN2007Identifier">
            <xs:annotation>
              <xs:documentation>Международный номер банковского счёта</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BICFI" type="BICFIIdentifier">
            <xs:annotation>
              <xs:documentation>Международный идентификатор банка</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:choice>
      <xs:element name="BankName" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Наименование банка</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BankAccountNumber" type="AccNumType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Кор.счет банка</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Currency" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код валюты</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PartyType">
    <xs:sequence>
      <xs:element name="PartyId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Id плательщика/получателя в системе клиента</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PartyExtId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Id плательщика/получателя в АБС банка</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" type="xs:string">
        <xs:annotation>
          <xs:documentation>Наименование плательщика (8) / получателя (16)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="INN" type="InnType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>ИНН плательщика (60) / получателя (61)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="KPP" type="KppType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>КПП плательщика (102) / получателя (103)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TransactionType">
    <xs:sequence>
      <xs:element name="TrnId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Id операции в системе клиента</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TrnExtId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Id операции в АБС банка</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OKUD" minOccurs="0">
        <xs:annotation>
          <xs:documentation>ОКУД (2)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:maxLength value="8"/>
            <xs:pattern value="\d{7,8}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="EntryDate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>Дата поступления в банк (62)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValueDate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>Дата валютирования (71)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DocNumber" type="xs:integer">
        <xs:annotation>
          <xs:documentation>Номер платежного поручения (3)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DocDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>Дата платежного поручения (4)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaymentKind">
        <xs:annotation>
          <xs:documentation>Вид платежа (5)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="0"/>
            <xs:enumeration value="почтой"/>
            <xs:enumeration value="электронно"/>
            <xs:enumeration value="телеграфом"/>
            <xs:enumeration value="срочно"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Payer" type="PartyType">
        <xs:annotation>
          <xs:documentation>Плательщик (8,60,102)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayerAccount" type="AccountType">
        <xs:annotation>
          <xs:documentation>Счет плательщика (9,10,11,12)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Payee" type="PartyType">
        <xs:annotation>
          <xs:documentation>Получатель (16,61,103)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayeeAccount" type="AccountType">
        <xs:annotation>
          <xs:documentation>Счет получателя (13,14,15,17)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Purpose" type="xs:string">
        <xs:annotation>
          <xs:documentation>Назначение операции (24)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayerAmount" type="xs:double" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сумма списания со счета плательщика (в валюте счета)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="xs:double">
        <xs:annotation>
          <xs:documentation>Эквивалентаня сумма операции (7)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayeeAmount" type="xs:double" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сумма зачисления на счет получателя (в валюте счета)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Priority" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Очередность платежа (21)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
            <xs:enumeration value="4"/>
            <xs:enumeration value="5"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PayType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Вид оп. (18)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:length value="2"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PayCode" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код (22)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DepartmentalInfo" type="DepartmentalInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Ведомственная Информация</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AddInfo" type="AddInfoType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="Id" type="xs:ID" use="optional"/>
  </xs:complexType>
  <xs:complexType name="PayDocType">
    <xs:sequence>
      <xs:element name="Transaction" type="TransactionType">
        <xs:annotation>
          <xs:documentation>Платежное поручение</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StampInfo" type="StampType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Информация для штампа</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AddInfo" type="AddInfoType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="Id" type="xs:ID" use="optional"/>
  </xs:complexType>
  <xs:complexType name="StampType">
    <xs:sequence>
      <xs:element name="OperatorFIO" type="xs:string"/>
      <xs:element name="FirstSignerFIO" type="xs:string" minOccurs="0"/>
      <xs:element name="SecondSignerFIO" type="xs:string" minOccurs="0"/>
      <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddInfoType">
    <xs:sequence>
      <xs:any namespace="##any" processContents="lax" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="InnType">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:maxLength value="12"/>
      <xs:pattern value="(0){1,12}"/>
      <xs:pattern value="(\d[1-9]|[1-9]\d)(\d[1-9]|[1-9]\d)(\d{4}[1-9]|\d{3}[1-9]\d|\d{2}[1-9]\d{2}|[1-9]\d{3}|[1-9]\d{4})\d"/>
      <xs:pattern value="(\d[1-9]|[1-9]\d)(\d[1-9]|[1-9]\d)(\d{5}[1-9]|\d{4}[1-9]\d|\d{3}[1-9]\d{2}|\d{2}[1-9]\d{3}|\d[1-9]\d{4}|[1-9]\d{5})\d{2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="KppType">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:maxLength value="9"/>
      <xs:pattern value="(0){1,9}"/>
      <xs:pattern value="(\d[1-9]|[1-9]\d)(\d[1-9]|[1-9]\d)([A-Z0-9][A-Z1-9]|[A-Z1-9][A-Z0-9])([1-9]\d{2}|\d{2}[1-9]|\d[1-9]\d)"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BicType">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:length value="9"/>
      <xs:pattern value="\d{9}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AccNumType">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:length value="20"/>
      <xs:pattern value="\d{20}"/>
    </xs:restriction>
