            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="AuthBankName">
                    <xs:pattern value=".*\S.*"/>
                    <xs:minLength value="1"/>
                    <xs:maxLength value="255"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
