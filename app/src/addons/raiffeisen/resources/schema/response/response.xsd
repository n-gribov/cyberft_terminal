                                <xs:documentation>Вторая часть номера</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Part3" type="Part3">
                            <xs:annotation>
                                <xs:documentation>Третья часть номера</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Part4" type="Part4" minOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Четвертая часть номера</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Part5" type="Part5" minOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Пятая часть номера</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="PsDate" type="xs:date" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Дата ПС</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="ResInfo">
                <xs:annotation>
                    <xs:documentation>1. Сведения о резиденте</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Name" type="OrgNameCC">
                            <xs:annotation>
                                <xs:documentation>1.1
                                    Наименование организации-резидента (вместе с наимен-ем формы собственности - то, что
                                    выводится на ПФ)
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Address">
                            <xs:annotation>
                                <xs:documentation>1.2</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="State" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Субъект РФ</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="District" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Район</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="City" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Город</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="PlaceType" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Тип населенного пункта</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Place" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Населенный пункт</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Street" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Улица</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Building" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Номер дома</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Block" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Корпус/строение</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Office" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Офис/квартира</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="OGRN" type="Ogrn">
                            <xs:annotation>
                                <xs:documentation>1.3 ОГРН организации-резидента</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Inn_Kpp">
                            <xs:annotation>
                                <xs:documentation>1.5
                                    ИНН или КПП
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:attribute name="inn" type="InnType">
                                    <xs:annotation>
                                        <xs:documentation>ИНН организации-резидента</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="kpp" type="KppType">
                                    <xs:annotation>
                                        <xs:documentation>КПП организации-резидента</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="BeneficiarInfo">
                <xs:annotation>
                    <xs:documentation>2. Реквизиты иностранного контрагента</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Beneficiar" type="BeneficiarInfo" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Реквизиты иностранного контрагента</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="ComData">
                <xs:annotation>
                    <xs:documentation>3.1 Общие сведения о контракте</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Num" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Номер контракта
                                    Либо 12#$%% либо б/н
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Date" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата подписания контракта</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Curr" type="DPCurr">
                            <xs:annotation>
                                <xs:documentation>Валюта цены контракта</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Sum" type="CurrAmount" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Сумма контракта</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="EndDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата завершения исполнения обязательств</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="SumTransfer" type="CurrAmount" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Сумма, подлежащая зачислению на счета за рубежом
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="SumCurr" type="CurrAmount" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Сумма, погашение которой предполагается за счет валютной выручки
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Tranche" type="PaymentPeriodCode" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Код срока привлечения

                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="checkNum" type="BoolType" use="required">
                        <xs:annotation>
                            <xs:documentation>Признак наличия номера контракта
                                1- отмечено, что без номера
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="checkSum" type="BoolType" use="required">
                        <xs:annotation>
                            <xs:documentation>Признак наличия суммы контракта
                                1- отмечено, что без суммы
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="TrancheInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>3.2 Сведения о сумме и сроках привлечения (предоставления) траншей по кредитному
                        договору
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Oper" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Sum" type="CurrAmount" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Сумма транша
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Code" type="PaymentPeriodCode" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Код срока привлечения транша
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Date" type="xs:date" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Ожидаемая дата поступления</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="RegInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>4. Сведения об оформлении, переводе и закрытии ПС</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Reg" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="RefClose" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Код основания закрытия ПС в соответствии с Инструкцией
                                                138-И
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="BankRegNum" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Регистрационный номер банка ПС</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="DateIn" type="xs:date" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Дата принятия ПС</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="DateClose" type="xs:date" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Дата закрытия ПС</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ReqNumber" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Порядковый номер табличной записи</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="RerInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>5. Сведения о переоформлении ПС</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Rer" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="RerNumber" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Номер переоформления</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="RerDate" type="xs:date" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Дата переоформления</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="RerRefNum" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Номер документа осеования</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="RerRefDate" type="xs:date" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Дата документа основания</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="NumPS" type="DealPassNumFullType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>6. Сведения о ранее оформленном паспорте сделки по контракту
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RefInfo">
                <xs:annotation>
                    <xs:documentation>7. Справочная информация</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ResInType" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>7.1 Способ предоставления резидентом документов для оформления
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ResInDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>7.1 Дата предоставления резидентом документов для оформления
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="RegSendType" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>7.2 Способ направления резиденту оформленного ПС</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ReqSendDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>7.2 Дата направления резиденту оформленного ПС</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="SpecData">
                <xs:annotation>
                    <xs:documentation>8. Специальные сведения о кредитном договоре</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Percent" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>8.1 Процентные платежи, предусмотренные кредитным договором (за искл.
                                    платежей по возврату основного долга)
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="FixPercent" type="xs:decimal" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>1. Фиксированный размер процентной ставки (% годовых)
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="LIBOR" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>2. Код ставки LIBOR
                                                например, Л06
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="OtherMethod" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>3. Другие методы определения процентной ставки
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IncrPercent" type="xs:decimal" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>4. Размер процентной надбавки (% годовых)
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="OtherPay" type="AddInfo" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    8.2 Иные платежи, предусмотренные кредитным договором (за искл. платежей по возврату
                                    основного долга и процентных платежей,

                                    указанный в п.8.1)
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="DebtSum" type="CurrAmount" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>8.3 Сумма задолженности по основному долгу на дату, предшествующую
                                    дате оформления ПС
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="HelpInfo">
                <xs:annotation>
                    <xs:documentation>9 Справочная информация о кредитном договоре</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Payments" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>9.1 и
                                    Основания для заполнения пункта 9.2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence minOccurs="0">
                                    <xs:element name="Return" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>9.2 Описание графика платежей по возврату основного долга
                                                и процентных платежей
                                            </xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence minOccurs="0">
                                                <xs:element name="strNum" type="xs:integer" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Номер п/п</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PercentInfo" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Суммы платежей по датам их
                                                            осуществления (В счет процентных
                                                            платежей)
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="Date" type="xs:date" minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Дата платежа
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="Sum" type="CurrAmount" minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Сумма платежа
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                                <xs:element name="DebtInfo" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Суммы платежей по датам их
                                                            осуществления (В счет основного
                                                            долга)
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="Date" type="xs:date" minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Дата платежа
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="Sum" type="CurrAmount" minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Сумма платежа
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                                <xs:element name="SpecConditions" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Описание особых условий</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                                <xs:attribute name="ground" type="BoolType" use="required">
                                    <xs:annotation>
                                        <xs:documentation>
                                            9.1 Основания для заполнения п.9.2

                                            0 - 9.1.1 Сведения из кредитного договора
                                            1 - 9.1.2 Оценочные сведения
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="DirectInvesting" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>9.3 Отметка о наличии прямого инвестирования</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:attribute name="check" type="BoolType" form="unqualified" use="required">
                                    <xs:annotation>
                                        <xs:documentation>0 - не отмечено</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="CredReceipt" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>9.5 Информация о привлечении резидентом кредита (займа),
                                    предоставленного нерезидентами на синдицированной
                                    (консорциональной) основе
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Oper" minOccurs="2" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="BeneficiarInfo" type="BeneficiarInfo">
                                                    <xs:annotation>
                                                        <xs:documentation>Реквизиты иностранного контрагента
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:choice>
                                                    <xs:annotation>
                                                        <xs:documentation>Сумма, предоставляемая по кредитному
                                                            договору
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:element name="Sum" type="CurrAmount">
                                                        <xs:annotation>
                                                            <xs:documentation>Сумма</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element name="Percent">
                                                        <xs:annotation>
                                                            <xs:documentation>Доля в общей сумме
                                                            </xs:documentation>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:decimal">
                                                                <xs:minExclusive value="0" />
                                                                <xs:maxExclusive value="100" />
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                </xs:choice>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="DepositSum" type="CurrAmount" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>9.4 Сумма залогового или другого обеспечения</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="AddInfo" type="AddInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Доп. информация</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LinkedDocs" type="LinkedDocs" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Связанные документы, например, реестр пополнения корп. карт, зарплатные ведомости
                        и т.п.
                    </xs:documentation>
                </xs:annotation>
