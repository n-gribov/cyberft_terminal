<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://bssys.com/upg/response"
           targetNamespace="http://bssys.com/upg/response" elementFormDefault="qualified"
           attributeFormDefault="unqualified">
    <xs:element name="Response" type="Response">
        <xs:annotation>
            <xs:documentation>Ответ на запрос УС</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="ServicePackage">
        <xs:sequence>
            <xs:element name="PackageId" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Идентификатор пакета услуг</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PackageName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование пакета услуг</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Service" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Информация об услуге в пакете услуг</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ServiceId" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Идентификатор услуги в Corrects</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ServiceName" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Наименование услуги</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:attributeGroup name="AddRests">
        <xs:annotation>
            <xs:documentation>Доступный остаток и т.п.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="ldgBal" type="xs:decimal">
            <xs:annotation>
                <xs:documentation>Доступный остаток</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ldgBalNat" type="xs:decimal">
            <xs:annotation>
                <xs:documentation>Доступный остаток в нац. валюте</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="BankDate">
        <xs:annotation>
            <xs:documentation>Прочие банковские даты по документу</xs:documentation>
        </xs:annotation>
        <xs:attribute name="chargeOffDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Списания со счета Плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="fileDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Постановки в картотеку</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="signDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Отметки банком Плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="receiptDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Поступило в банк Плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dpp" type="xs:date">
            <xs:annotation>
                <xs:documentation>Дата перечисления платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="recDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Дата отметки банка Получателя</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="PayeeBank">
        <xs:annotation>
            <xs:documentation>Банк получателя</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payeeBankName">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="140" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="payeeBankBic" type="BIC" />
        <xs:attribute name="payeeBankCorrAcc" type="AccComType" />
    </xs:attributeGroup>
    <xs:attributeGroup name="PayerBank">
        <xs:annotation>
            <xs:documentation>Банк плательщика</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payerBankName">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="140" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="payerBankCorrAcc" type="AccComType" />
        <xs:attribute name="payerBankBic" type="BIC" />
    </xs:attributeGroup>
    <xs:attributeGroup name="Payer">
        <xs:annotation>
            <xs:documentation>Реквизиты плательщика</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payerINN" type="InnType" use="optional">
            <xs:annotation>
                <xs:documentation>ИНН плательщика (до 12)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="payerAcc" type="AccComType" use="required">
            <xs:annotation>
                <xs:documentation>Номер счета плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="payerName" use="optional">
            <xs:annotation>
                <xs:documentation>Наименование плательщика</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="300" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="payerKPP" type="KppType">
            <xs:annotation>
                <xs:documentation>КПП плательщика.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="Payee">
        <xs:annotation>
            <xs:documentation>Реквизиты получателя</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payeeINN" type="InnType" use="optional" />
        <xs:attribute name="payeeAcc" type="AccComType" use="required" />
        <xs:attribute name="payeeName" use="optional">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="300" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="payeeKPP" type="KppType">
            <xs:annotation>
                <xs:documentation>КПП получателя.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="ComTransInfo">
        <xs:attribute name="dc" type="BoolType" use="required">
            <xs:annotation>
                <xs:documentation>Признак дебетового/кредитового приложения</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docNum" use="required">
            <xs:annotation>
                <xs:documentation>Номер расчетного документа</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="16" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="docDate" type="xs:dateTime" use="optional">
            <xs:annotation>
                <xs:documentation>Дата расчетного документа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docCurr" type="CurrCodeDigitalType">
            <xs:annotation>
                <xs:documentation>Валюта платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docSum" type="xs:decimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма документа в валюте платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docSumNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Сумма документа в рублях</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="purpose" use="optional">
            <xs:annotation>
                <xs:documentation>Назначение платежа</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="300" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="branchCode" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Код подразделения Сбербанка</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="paymentOrder" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation>Очередность платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="paytKind" type="PayKindType" use="optional">
            <xs:annotation>
                <xs:documentation>Вид платежа
                    Возможные значения: «почтой», «электронно», «телеграфом» («срочно» временно недоступно).
                    "0" - есои не указано
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="transKind" use="required">
            <xs:annotation>
                <xs:documentation>Вид операции</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:length value="2" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="bankNumDoc">
            <xs:annotation>
                <xs:documentation>Номер документа банка (до 20 символов)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1" />
                    <xs:maxLength value="20" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="carryDate" type="xs:dateTime" use="required">
            <xs:annotation>
                <xs:documentation>Дата проводки</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="writeOffDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>Дата списания со счета плательщика
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:annotation>
        <xs:documentation>Общие для всех взаимодействий сложные типы</xs:documentation>
    </xs:annotation>
    <xs:complexType name="LinkedDocs">
        <xs:annotation>
            <xs:documentation>Связанные документы</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="LDoc" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Документ, связанный с поручением</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence minOccurs="0">
                        <xs:element name="Params" type="Params" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дополнительные параметры связанного с поручением документа
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="type">
                        <xs:annotation>
                            <xs:documentation>Тип связанного документа</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="1" />
                                <xs:maxLength value="50" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="docExtId" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation>Идентификатор документа в УС</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:annotation>
        <xs:documentation>Сложные типы, используемые в валютном переводе</xs:documentation>
    </xs:annotation>
    <xs:annotation>
        <xs:documentation>Сложные типы взаимодействий релизов 0 - 1</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup name="OrgDataAttr">
        <xs:annotation>
            <xs:documentation>ИНН, КПП в виде атрибутов</xs:documentation>
        </xs:annotation>
        <xs:attribute name="inn" use="required">
            <xs:annotation>
                <xs:documentation>ИНН (до 12)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="InnType">
                    <xs:pattern value="[0-9]{5,12}" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="kpp" type="KppType">
            <xs:annotation>
                <xs:documentation>КПП (до 9)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="ComRests">
        <xs:annotation>
            <xs:documentation>Информация об остатках на счете</xs:documentation>
        </xs:annotation>
        <xs:attribute name="stmtDateTime" type="xs:dateTime" use="required">
            <xs:annotation>
                <xs:documentation>Дата и время, на которые была сформирована выписка</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="bic" type="BicType" use="required" />
        <xs:attribute name="lastMovetDate" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation>Дата последней операции по счету за запрошеный период</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="datePLast" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation>Дата предыдущей операции за запрошеный период
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="acc" type="AccNumType" use="required">
            <xs:annotation>
                <xs:documentation>Номер счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rateIn" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Курс ЦБ за 1 ед. валюты на начало периода.
                    Заполняется только для счетов и ин. валюте
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rateOut" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Курс ЦБ за 1 ед. валюты на конец периода.
                    Заполняется только для счетов и ин. валюте
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="enterBal" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Входящий остаток в валюте счета
                    Если запрашивались остатки, то на начало опер. дня
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="enterBalNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Входящий остаток в нац. валюте
                    Если запрашивались остатки, то на начало опер. дня
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="outBal" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Исходящий остаток в валюте счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="outBalNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Исходящий остаток в нац. валюте</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="planOutBal" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Плановый в валюте счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="planOutBalNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Плановый в нац. валюте</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="beginDate" type="xs:date" use="required">
            <xs:annotation>
                <xs:documentation>Дата начала периода</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="endDate" type="xs:date" use="required">
            <xs:annotation>
                <xs:documentation>Дата окончания периода</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="stmtType" use="required">
            <xs:annotation>
                <xs:documentation>Тип запроса выписки: 6 - только итоговая, 101 - только плановая, 106 - итоговая +
                    плановая, 1 - не итоговая, не
                    плановая (если в банке используется один вид) 0 - остатки
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1" />
                    <xs:maxLength value="3" />
                    <xs:enumeration value="1" />
                    <xs:enumeration value="6" />
                    <xs:enumeration value="101" />
                    <xs:enumeration value="106" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="requestDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Дата запроса выписки</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:complexType name="Params">
        <xs:annotation>
            <xs:documentation>Множество параметров произвольного вида</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Param" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Параметр</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="Param" />
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Param">
        <xs:annotation>
            <xs:documentation>Отдельный параметр произвольного вида</xs:documentation>
        </xs:annotation>
        <xs:attribute name="name" use="required">
            <xs:annotation>
                <xs:documentation>Название параметра</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1" />
                    <xs:maxLength value="255" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="value" use="required">
            <xs:annotation>
                <xs:documentation>Значение параметра</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1" />
                    <xs:maxLength value="255" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="Account" mixed="true">
        <xs:annotation>
            <xs:documentation>Номер счёта в виде значения элемента и БИК в значении параметра</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="AccNumType">
                <xs:attribute name="bic" type="BicType" use="required" />
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="Bank">
        <xs:sequence>
            <xs:element name="Name" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Наименование банка</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="80" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="BankCity" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Название населённого пункта банка</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="30" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SettlementType" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Тип населённого пункта банка</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="20" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="bic" type="BicType" use="required">
            <xs:annotation>
                <xs:documentation>БИК</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="correspAcc" type="AccNumType" use="optional">
            <xs:annotation>
                <xs:documentation>Номер корр. счёта банка</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="DigitalSign">
        <xs:sequence>
            <xs:element name="Issuer" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор поставщика сертификата
                        Например, " issuer="E=IdleCA, C=RU, S=Idle CA, L=Idle CA, O=Idle CA, OU=Idle CA, CN=Idle CA" "
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="1024" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SN">
                <xs:annotation>
                    <xs:documentation>Серийный номер сертификата</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="255" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Value" type="xs:base64Binary">
                <xs:annotation>
                    <xs:documentation>Значение ЭП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DigestName" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Имя схемы подписи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DigestVersion" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Версия схемы подписи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SignType" type="xs:string" />
            <xs:element name="Fio" type="xs:string" minOccurs="0" />
            <xs:element name="Position" type="xs:string" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>
    <xs:annotation>
        <xs:documentation>Сложные типы из взаимодействий релиза 2</xs:documentation>
    </xs:annotation>
    <xs:complexType name="CurrAmountType">
        <xs:annotation>
            <xs:documentation>Денежная сумма с указанием трёхчисленного кода валюты</xs:documentation>
        </xs:annotation>
        <xs:attribute name="docSum" type="xs:decimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма</xs:documentation>
            </xs:annotation>
        </xs:attribute>
