    <xs:complexType name="FailReasonsEntry">
        <xs:annotation>
            <xs:documentation>Описание строки репликационного пакета «справочника формализованных причин отказа» в ЕКС</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Entry">
                <xs:attribute name="code" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:documentation>Код причины</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="inPartDocsSubmitted" type="xs:boolean" use="optional">
                    <xs:annotation>
                        <xs:documentation>В части представленных документов</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="docType" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:documentation>Тип документа валютного контроля</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="field" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:documentation>Поле документа</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="comment" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:documentation>Правило заполнения/замечания</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

