    <xs:complexType name="ExpressToolsOptionsEntry">
        <xs:annotation>
            <xs:documentation>История изменений видов срочных инструментов</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Entry">
                <xs:attribute name="tool" use="optional">
                    <xs:annotation>
                        <xs:documentation>Наименование инструмента. Например, "Депозит Отзываемый Онлайн"</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="operationKind" use="optional">
                    <xs:annotation>
                        <xs:documentation>Наименование вида операции.</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="350"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="daysFrom" use="optional">
                    <xs:annotation>
                        <xs:documentation>Кол-во дней, которое относится к началу диапазона доступности операции и участвует в алгоритме расчета даты начала диапазона</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:int">
                            <xs:minInclusive value="0"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="daysTo" use="optional">
                    <xs:annotation>
                        <xs:documentation>Кол-во дней, которое относится к началу диапазона доступности операции и участвует в алгоритме расчета даты начала диапазона</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:int">
                            <xs:minInclusive value="0"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="operPossibly" use="optional">
                    <xs:annotation>
                        <xs:documentation>Признак доступности операции для инструмента. Возможные значения: 0 - недоступна, 1 - доступна</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:boolean">
                            <xs:pattern value="0|1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="sumMin" type="xs:decimal" use="optional">
                    <xs:annotation>
                        <xs:documentation>Размер минимальной суммы для операции</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="sumMax" type="xs:decimal" use="optional">
                    <xs:annotation>
                        <xs:documentation>Размер максимальной суммы для операции</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="DictsForDeposits">
