<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://bssys.com/upg/response"
           targetNamespace="http://bssys.com/upg/response" elementFormDefault="qualified"
           attributeFormDefault="unqualified" version="1.0">

    <xs:include schemaLocation="common-types.xsd"/>
    <xs:include schemaLocation="organization-info.xsd"/>

    <xs:element name="Response" type="Response">
        <xs:annotation>
            <xs:documentation>Ответ СББОЛ на запрос УС</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:attributeGroup name="BankDate">
        <xs:annotation>
            <xs:documentation>Прочие банковские даты по документу</xs:documentation>
        </xs:annotation>
        <xs:attribute name="chargeOffDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Списания со счета Плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="fileDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Постановки в картотеку</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="signDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Отметки банком Плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="receiptDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Поступило в банк Плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dpp" type="xs:date">
            <xs:annotation>
                <xs:documentation>Дата перечисления платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="recDate" type="xs:date">
            <xs:annotation>
                <xs:documentation>Дата отметки банка Получателя</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="PayeeBank">
        <xs:annotation>
            <xs:documentation>Банк получателя</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payeeBankName" type="xs:string"/>
        <xs:attribute name="payeeBankBic" type="xs:string"/>
        <xs:attribute name="payeeBankCorrAcc" type="xs:string"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="PayerBank">
        <xs:annotation>
            <xs:documentation>Банк плательщика</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payerBankName" type="xs:string"/>
        <xs:attribute name="payerBankCorrAcc" type="xs:string"/>
        <xs:attribute name="payerBankBic" type="xs:string"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="Payer">
        <xs:annotation>
            <xs:documentation>Реквизиты плательщика</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payerINN" type="InnType" use="optional">
            <xs:annotation>
                <xs:documentation>ИНН плательщика (до 12)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="payerAcc" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Номер счета плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="payerName" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Наименование плательщика</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="payerKPP" type="xs:string">
            <xs:annotation>
                <xs:documentation>КПП плательщика.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="Payee">
        <xs:annotation>
            <xs:documentation>Реквизиты получателя</xs:documentation>
        </xs:annotation>
        <xs:attribute name="payeeINN" type="InnType" use="optional"/>
        <xs:attribute name="payeeAcc" type="xs:string" use="required"/>
        <xs:attribute name="payeeName" type="xs:string" use="optional"/>
        <xs:attribute name="payeeKPP" type="xs:string">
            <xs:annotation>
                <xs:documentation>КПП получателя.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="ComTransInfo">
        <xs:attribute name="dc" type="BoolType" use="required">
            <xs:annotation>
                <xs:documentation>Признак дебетового/кредитового приложения</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docNum" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Номер расчетного документа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docDate" type="xs:dateTime" use="optional">
            <xs:annotation>
                <xs:documentation>Дата расчетного документа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docCurr" type="CurrCodeDigitalType">
            <xs:annotation>
                <xs:documentation>Валюта платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docSum" type="xs:decimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма документа в валюте платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="docSumNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Сумма документа в рублях</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="purpose" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Назначение платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="branchCode" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Код подразделения Сбербанка</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="paymentOrder" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation>Очередность платежа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="paytKind" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Вид платежа
                    Возможные значения: «почтой», «электронно», «телеграфом» («срочно» временно недоступно).
                    "0" - если не указано
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="transKind" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Вид операции</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="bankNumDoc" type="xs:string">
            <xs:annotation>
                <xs:documentation>Номер документа банка (до 20 символов)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="carryDate" type="xs:dateTime" use="required">
            <xs:annotation>
                <xs:documentation>Дата проводки</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="urgentSBRF" type="xs:string">
            <xs:annotation>
                <xs:documentation>Дополнительная услуга СБРФ.
                    Срочность: 0 - срочный, 1 - срочный с УВ, 2 - неотложный
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:complexType name="LinkedDocs">
        <xs:annotation>
            <xs:documentation>Связанные документы</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="LDoc" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Документ, связанный с поручением</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence minOccurs="0">
                        <xs:element name="Params" type="Params" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дополнительные параметры связанного с поручением документа
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="type" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Тип связанного документа</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="docExtId" type="UuidSeparated" use="optional">
                        <xs:annotation>
                            <xs:documentation>Идентификатор документа в УС</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="docId" type="UuidSeparated">
                        <xs:annotation>
                            <xs:documentation>Глобальный идентификатор документа</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:attributeGroup name="OrgDataAttr">
        <xs:annotation>
            <xs:documentation>ИНН, КПП в виде атрибутов</xs:documentation>
        </xs:annotation>
        <xs:attribute name="inn" type="InnType" use="required">
            <xs:annotation>
                <xs:documentation>ИНН (до 12)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="kpp" type="xs:string">
            <xs:annotation>
                <xs:documentation>КПП (до 9)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="ComRests">
        <xs:annotation>
            <xs:documentation>Информация об остатках на счете</xs:documentation>
        </xs:annotation>
        <xs:attribute name="stmtDateTime" type="xs:dateTime" use="required">
            <xs:annotation>
                <xs:documentation>Дата и время, на которые была сформирована выписка</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="bic" type="xs:string" use="required"/>
        <xs:attribute name="lastMovetDate" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation>Дата последней операции по счету за запрошеный период</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="datePLast" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation>Дата предыдущей операции за запрошеный период
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="acc" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Номер счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rateIn" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Курс ЦБ за 1 ед. валюты на начало периода.
                    Заполняется только для счетов и ин. валюте
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rateOut" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Курс ЦБ за 1 ед. валюты на конец периода.
                    Заполняется только для счетов и ин. валюте
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="enterBal" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Входящий остаток в валюте счета
                    Если запрашивались остатки, то на начало опер. дня
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="enterBalNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Входящий остаток в нац. валюте
                    Если запрашивались остатки, то на начало опер. дня
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="outBal" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Исходящий остаток в валюте счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="outBalNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Исходящий остаток в нац. валюте</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="planOutBal" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Плановый в валюте счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="planOutBalNat" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Плановый в нац. валюте</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="beginDate" type="xs:date" use="required">
            <xs:annotation>
                <xs:documentation>Дата начала периода</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="endDate" type="xs:date" use="required">
            <xs:annotation>
                <xs:documentation>Дата окончания периода</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="stmtType" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Тип запроса выписки: 6 - только итоговая, 101 - только плановая, 106 - итоговая +
                    плановая, 1 - не итоговая, не
                    плановая (если в банке используется один вид) 0 - остатки
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:complexType name="Params">
        <xs:annotation>
            <xs:documentation>Множество параметров произвольного вида</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Param" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Параметр</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="Param"/>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Param">
        <xs:annotation>
            <xs:documentation>Отдельный параметр произвольного вида</xs:documentation>
        </xs:annotation>
        <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Название параметра</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="value" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Значение параметра</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="Account">
        <xs:annotation>
            <xs:documentation>Номер счёта в виде значения элемента и БИК в значении параметра</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="bic" type="xs:string" use="required"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="RZKPayDocRu">
        <xs:sequence>
            <xs:element name="AccDoc" type="AccDoc" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Реквизиты платёжного документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Payer" type="Client" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Реквизиты плательщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Payee" type="Contragent" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Реквизиты получателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DepartmentalInfo" type="DepartmentalInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Налоговая информация: поля 101, 104 - 110 платёжного поручения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Information" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Информация получателю</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Credit" type="Credit" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Доп. реквизиты. Указываются для платёжных документов, проводимых за счёт
                        предоставляемого банком кредита
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Rzk" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Многострочная аналитика и параметры обработки платежного документа в РЦК (СБК)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="RzkParams" type="RzkParams" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Параметры обработки документа в РЦК (СБК)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="RzkDocanalytics" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Многострочная аналитика платежного документа в соответствии со справочниками РЦК (СБК)</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="RzkDocanalytic" minOccurs="0" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>Строка многострочной аналитики платежного поручения</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="FieldSetId" type="xs:string" nillable="false" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор записи многострочной аналитики</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="TableRow" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Номер строки Многострочной аналитики</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="DocContentId" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор документа (Платежного поручение), к которому относится строка многострочной аналитики</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Amount" type="xs:decimal" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Сумма</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="BudgetId" type="xs:long" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор структуры</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Classifier1Id" type="xs:long" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор сметы доходов и расходов ЦО</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Class1Code" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Код сметы доходов и расходов ЦО</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Class1Description" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Наименование сметы доходов и расходов ЦО</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="FinanceTypeDescription" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Название вида финансирования</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="FinanceTypeId" type="xs:long" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор вида финансирования в РЦК (СБК)</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="KesrCode" type="xs:long" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Классификатор сметы</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="KesrDescription" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Наименование сметы статей доходов и расходов</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="OrgGroupCaption" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Наименование группы организаций</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="OrgGroupId" type="xs:long" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор группы организаций/</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PactDate" type="xs:date" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Дата договора</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PactId" type="xs:long" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор договора в РЦК (СБК)</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PactNum" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Номер договора</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PactSubject" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Наименование договора</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PayCodeCode" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Код вида расчетов</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PayCodeDescription" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Название вида расчёта</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PayCodeId" type="xs:long" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Идентификатор вида расчёта в РЦК (СБК)</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="ProjectStdCode" type="xs:string" nillable="true">
                                                    <xs:annotation>
                                                        <xs:documentation>Код проекта</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="ProjectStdDescription" type="xs:string" nillable="true">
                                                    <xs:annotation>
