<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://bssys.com/upg/response"
           targetNamespace="http://bssys.com/upg/response" elementFormDefault="qualified"
           attributeFormDefault="unqualified" version="1.0">

    <xs:include schemaLocation="common-types.xsd"/>

    <xs:complexType name="OrganizationInfo">
        <xs:sequence>
            <xs:element name="OrgData" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Данные по организации и счетам
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="OrgId" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Идентификатор организации в СББОЛ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="OrgForm" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Организационно-правовая форма (текст, например, ООО)
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ShortName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Сокращенное наименование организации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="FinancialName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Финансовое наименование организации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="FullName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Полное наименование

                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="VkFullName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Полное наименование для ВК</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Accounts" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Счета организации доступные пользователю

                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Account" type="AccountRubType" minOccurs="0"
                                                maxOccurs="unbounded"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="StateType" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак рез/нерезидент</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="StateCode" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Тип (рез/нерезидент) (текст, например, резидент – физ.лицо)
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="OGRN" type="OgrnType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>ОГРН</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="DateOGRN" type="xs:date" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дата регистрации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="INN" type="InnType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>ИНН/КИО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="OKPO" type="OkpoType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>ОКПО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="OKATO" type="OkatoType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>ОКАТО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="BranchId" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Подразделение Банка, к которому приписана организация
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ExportImportUFEBS" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак принадлежности организации УФК: 0 – (FALSE), 1 –(TRUE).
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Addresses" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Адреса (все заведенные для организации)

                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Address" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="AddressTypeCode" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Код наименования типа адреса

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="AddressType" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Наименование типа

                                                            адреса (например, Юридический

                                                            адрес)

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Country" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>ISO-код страны (3-х символьный код страны)

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="CountryName" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Наименование страны

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="CountryShortName" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Краткое наименование страны

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Zip" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Индекс

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Sub" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Субъект/Регион

                                                            (полное наименование (так

                                                            храниться в справочнике адресов

                                                            организаций в СББОЛ)

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Area" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Район

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="City" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Город

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="SettlType" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Тип. нас. пункта

                                                            (например, РП)

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="SettlName" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Наименование нас.

                                                            пункта

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Str" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Улица

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="HNumber" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Номер дома

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Corp" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Корпус

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Office" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Офис

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="FullAddress" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Полный адрес

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="AddInfoAddress" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Примечание к

                                                            адресу

                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="OrgBranches" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Подразделения банка, в которых обслуживается организация
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="OrgBranch" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="BlockInfo" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Информация о приостановлении обслуживания в
                                                            системе ДБО
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="StopedService" type="BoolType"
                                                                        minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Приостановление оказания услуг:
                                                                        1- установлено
                                                                        0- не установлено
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="StopInfo" type="xs:string" minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Причина приостановления услуг
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="FinBlock" type="BoolType" minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Финансовая блокировка:
                                                                        1- установлена
                                                                        0- не установлена
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="FinBlockInfo" type="xs:string"
                                                                        minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Причина финансовой блокировки
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="ContractRescission" type="BoolType"
                                                                        minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Расторжение контракта (договора) с
                                                                        банком:
                                                                        1- установлено
                                                                        0- не установлено
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="ContractRescissionInfo" type="xs:string"
                                                                        minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Причина расторжения договора
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="DateRescission" type="xs:date"
                                                                        minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Дата расторжения контракта
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                            <xs:element name="Reason" type="xs:string" minOccurs="0">
                                                                <xs:annotation>
                                                                    <xs:documentation>Причина расторжения контракта
                                                                        (договора)
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                                <xs:element name="ServicePacks" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Услуги</xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="ServicePack" minOccurs="0"
                                                                        maxOccurs="unbounded">
                                                                <xs:complexType>
                                                                    <xs:attribute name="packageId" type="xs:string"
                                                                                  use="required">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Идентификатор пакета
                                                                                услуг
                                                                            </xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:attribute>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                                <xs:element name="CreditContracts" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Кредитные договора для данной организации
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="CreditContract" minOccurs="0"
                                                                        maxOccurs="unbounded">
                                                                <xs:complexType>
                                                                    <xs:attribute name="credContrtId" type="xs:string"
                                                                                  use="required">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Идентификатор кредитного
                                                                                договора
                                                                            </xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:attribute>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                                <xs:element name="SalaryContracts" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Перечень зарплатных договоров для данной
                                                            организации
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="SalaryContract" minOccurs="0"
                                                                        maxOccurs="unbounded">
                                                                <xs:annotation>
                                                                    <xs:documentation>Содержит информацию одного зп
                                                                        договора
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                                <xs:complexType>
                                                                    <xs:attribute name="contrID" type="UuidSeparated"
                                                                                  use="required">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Идентификатор договора
                                                                            </xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:attribute>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                                <xs:element name="ImprestAcc" type="BoolType" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Признак "Получать выписки по ссудным счетам"
                                                            1 - признак установлен
                                                            0 - не установлен
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="branchId" type="xs:string" use="required">
                                                <xs:annotation>
                                                    <xs:documentation>Идентификатор подразделения банка
                                                    </xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="contractNum" type="xs:string">
                                                <xs:annotation>
                                                    <xs:documentation>Номер договора</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="contractDate" type="xs:date">
                                                <xs:annotation>
                                                    <xs:documentation>Дата договора</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="connectionDate" type="xs:date">
                                                <xs:annotation>
                                                    <xs:documentation>Дата подключения</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="OtherOrgData" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Прочие данные по организации</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="InternationalName" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Полное международное наименование организации
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IntShortName" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Сокращенное международное наименование</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="NameInDocs" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Наименование организации в документах</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="UseNameInDocs" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Использовать наименование организации в документах
                                                0 - не использовать
                                                1 - использовать
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Locked" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Признак блокировки/ приостановки обслуживания организации
                                                по системе ДБО

                                                Возможные значения: 0-признак не установлен, 1 – признак установлен

                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="DboUse" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Признак "Не обслуживается по системе ДБО"
                                                1 - не обслуживается
                                                0 - обслуживается
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="OrgKpp" type="OrgKpp" minOccurs="0" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>Список КПП</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="DictContrUse" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Признак "Использовать подтвержденный справочник
                                                контрагентов"
                                                1 - использовать (чек выставлен)
                                                0 - не использовать
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Frod" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Признак "ФРОД-мониторинг"
                                                1 - используется
                                                0 - не используется
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="E-invoising" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Признак "E-invoicing"
                                                1 - используется
                                                0 - не используется
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="CertAuthId" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Код удостоверяющего центра. Идентификатор организации в
                                                удостоверяющем центре.
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="LastCertifNum" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Номер последнего порядкового номера сертификата в
                                                удостоверяющем центре
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Contacts" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Содержит информацию о контактных лицах организации
                                            </xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="DirectorFio" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>ФИО руководителя</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="DirectorTel" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Телефон руководителя</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="AccountantFio" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>ФИО главного бухгалтера</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="AccountantTel" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Телефон главного бухгалтера</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Fax" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Факс</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Email" type="xs:string" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>Адрес электронной почты</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="AddInfo" type="xs:string" minOccurs="0"/>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="CurrControlSettings" type="CurrControlSettings" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Настройки валютного контроля</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="AdmContract" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Договоры внесения средств</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="USDetails" minOccurs="0" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>Реквизиты договора самоинкосации
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
