<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://bssys.com/upg/response"
           targetNamespace="http://bssys.com/upg/response" elementFormDefault="qualified"
           attributeFormDefault="unqualified" version="1.0">

    <xs:complexType name="AccountRubType">
        <xs:annotation>
            <xs:documentation>Реквизиты счёта</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Bank" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Реквизиты банка зачисления</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Name" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Наименование банка (наименование отделения СБРФ)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="BankName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Наименование банка по БИК РФ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="BankCity" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Населённый пункт банка зачисления</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="SettlementType" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Тип населённого пункта банка зачисления</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="bic" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>БИК банка зачисления рублей</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="obsnum" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>OBSNUM</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="OtherAccountData" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Прочие данные по счетам</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Name" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Наименование счета</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="CurrencyCode" type="CurrCodeDigitalType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Числовой код валюты счета

                                    Если не пришло, то вычисляем по номеру счета
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="CurrCode" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>ISO-код валюты договора (3-х буквенный код валюты)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="UseOrgNameInDoc" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Использовать наименование организации в документах
                                    1 - использовать
                                    0 - не использовать
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="OrgNameInDoc" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Наименование орагнизации в документах</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="AccountType" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Тип счёта: 01 – расчетный, 02 – бюджетный, 03 – транзитный, 04- специальный транзитный валютный счет, 05 - ссудный, 06 - счет по учету обеспечения.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="AccountTypeP" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Тип счёта: 0 – активный, 1 – пассивный (по умолчанию)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="DBO" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Обслуживание в ДБО
                                    1 - обслуживается
                                    0 - не обслуживается
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Business" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак бизнес-счёта</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="IsBusinessNewType" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак бизнес-счёта "нового" типа</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="IsNotDelay" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак возможности проведения неотложных платежей: 0 - не установлен, 1 - установлен</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="IsUrgent" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак проведения срочных платежей
                                    Возможные значения: 0 - не установлен, 1 - установлен
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="IsIntermed" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак возможности предоставления информации о платежах по счету третьим лицам: 0 - не установлен, 1 - установлен</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="CreateDate" type="xs:date" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дата открытия счета</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="CloseDate" type="xs:date" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дата закрытия счёта</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="State" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Состояние счета:
                                    OPEN - открыт;
                                    BLOCKED - заблокирован;
                                    CLOSED - закрыт.
                                    По умолчанию - OPEN
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Mode" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Режим:
                                    1 - стандартный;
                                    2 - разрешена подготовка только выписок по счету (запрещен прием документов);
                                    3 - разрешен только прием документов (запрещено формирование выписок).
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="BlockedFullInfo" type="BlockedFullInfo" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Блокировки средств на счетах</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="CartotecaDocInfo" type="CartotecaDoc" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Информация о документах, поставленных в Картотеку 1 и 2 к счету</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="MinBalance" type="xs:decimal" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Минимальный поддерживаемый остаток на счете (неснижаемый остаток)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="SumOvd" type="xs:decimal" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Сумма общего лимита овердрафта в валюте счёта</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="AddInfo" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дополнительная информация</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="AddInfoSBK" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="AccClientInfo" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к договору о предосталении информации по счетам клиента</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ControlOper" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к договору о предосталении информации для контроля операций по счетам клиента</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="AcceptPP" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к дополнителдьному согласшению к договору банковского счета о контроле и акцепте платежных документов</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="DebetLimit" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к соглашению о лимите дебетового счета</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="SingleResidue" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к соглашению о предосталении услуги "Единый остаток"</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="AddAgree" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Заключено доп соглашение к договору банковского счета по Единому остатку</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Overdraft" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к соглашению о предосталении услуги овердрафт с общим лимитом</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="DirectControl" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к договору о прямом управлении счетами</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="AddDirectControl" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к дополнительному соглашению к договору банковского счета о прямом управлении счетами</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="RzkId" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Идентификатор счета в РЦК (во внешней системе)</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="RzkOrgAccTypeId" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Идентификатор типа счета в РЦК (во внешней системе)</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="RzkOrgId" type="xs:string" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Идентификатор организации счета в РЦК (во внешней системе)</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="CompensatingProducts" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="AccContract" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Подключение счета к договору компенсационного продукта</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="CreditLimitSum" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Залючено допоонительное соглашение по компенсационному продукту "Начисление процентов на сумму кредитного остатка"</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="MiddleLimit" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Залючено допоонительное соглашение по компенсационному продукту "Начисление процентов на совокупный (среднехронологический) остаток"</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="MinimumBalance" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Залючено допоонительное соглашение по компенсационному продукту "Начисление процентов на совокупный неснижаемый остаток"</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Overdraft" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Залючено допоонительное соглашение по компенсационному продукту "Начисление процентов на сумму остатка овердрафтного кредита"</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="CreditLimit" type="BoolType" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>Залючено допоонительное соглашение по компенсационному продукту "Начисление процентов на сумму кредитного остатка и остатка овердрафтного кредита"</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="CreditContracts" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Если счет является ссудным, дополнительно может передаваться информация по кредитным договорам.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="CreditContract" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:attribute name="credContrtId" type="xs:string" use="required">
                                                <xs:annotation>
                                                    <xs:documentation>Идентификатор кредитного договора</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="ContractOfTheStateDefenseOrder" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Номер договора государственного оборонного заказа (ГОЗ)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Adm" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Счет подключен к устройству
                                    самообслуживания на территории
                                    Клиента (АДМ)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="SelfEnCashment" type="BoolType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Счет подключен к устройству
                                    самообслуживания (УС)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="accountId" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Идентификатор счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accountType" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Тип счета</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="accNum" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Номер счёта зачисления</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="CartotecaDoc">
        <xs:sequence minOccurs="0">
            <xs:element name="PermitionOperDocs" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Содержит информацию о расчетных документах, ожидающих разрешения на проведение
                        операции
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="quantity" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество документов</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="sum" type="xs:decimal" use="required">
                        <xs:annotation>
                            <xs:documentation>На сумму.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="AcceptDocs" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Содержит информацию о расчетных документах, ожидающих акцепта</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="quantity" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество документов</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="sum" type="xs:decimal" use="required">
                        <xs:annotation>
                            <xs:documentation>На сумму.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="Cartoteca2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Содержит информацию о расчетных документах, помещённых в картотеку к счету 90902
                        (картотека 2)
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="quantity" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество документов</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="sum" type="xs:decimal" use="required">
                        <xs:annotation>
                            <xs:documentation>На сумму.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="OperAccept" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документы, ожидающих разрешения на проведение операции в рублях РФ (картотека 1) –
                        Только для валютных счетов.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="quantity" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество документов</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="sum" type="xs:decimal" use="required">
                        <xs:annotation>
                            <xs:documentation>Сумма в валюте Рубли РФ</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="NoMoney" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документы, помещённые в картотеку из-за отсутствия\недостаточности денежных
                        средств на счете клиента в рублях РФ(картотека 2) – Только для валютных счетов.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="quantity" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество документов</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="sum" type="xs:decimal" use="required">
                        <xs:annotation>
                            <xs:documentation>Сумма в валюте Рубли РФ</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BlockedFullInfo">
        <xs:sequence>
            <xs:element name="Debet" type="DebetFull" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Блокировки по дебету</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Cred" type="CredFull" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Блокировки по кредиту</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CredFull">
        <xs:attribute name="check" type="BoolType">
            <xs:annotation>
                <xs:documentation>1 - установлена блокировка по кредиту, 0 - не установлена</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="annotation" type="xs:string">
            <xs:annotation>
                <xs:documentation>Основание ареста</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="arrestedBy" type="xs:string">
            <xs:annotation>
                <xs:documentation>Наименование органа, наложившего арест</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="arestedByNum" type="xs:string">
            <xs:annotation>
                <xs:documentation>Код налогового органа, наложившего арест</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="beginDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>Дата начала действия ограничения</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="endDate" type="xs:dateTime">
            <xs:annotation>
                <xs:documentation>Дата снятия ограничения</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="DebetFull">
        <xs:sequence>
            <xs:element name="BlockedSumInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Аресты по счету (Блокировки по сумме)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="BlockedSum" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Заблокированные (арестованные) суммы на счёте.</xs:documentation>
                            </xs:annotation>
